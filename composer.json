{
  "name": "linkuha/db",
  "type": "library",
  "description" : "DB management assistance classes",
  "keywords": [
    "database",
    "PDO",
    "mysqli",
    "handlersocket"
  ],
  "license": "MIT",
  "homepage": "https://github.com/linkuha/php-db",
  "authors": [
    {
      "name" : "Pudich Alexandr",
      "email" : "linkuha@gmail.com",
      "homepage" : "https://pudich.ru"
    }
  ],
  "require": {
    "php" : ">=5.6.0",

    "lib-pcre": "*",
    "ext-mysqli": "*",
    "ext-PDO": "*",
    "ext-pdo_mysql": "*",
    "ext-handlersocket": "*"
  },
  "require-dev": {
  },
  "autoload": {
    "psr-4": {
      "SimpleLibs\\Db\\": "src"
    }
  },
  "autoload-dev": {
  },
  "suggest": {
    "linkuha/php-helpers": "Helpers contain random_int polyfill for PHP 5.6."
  },
  "scripts": {
  },
  "config": {
    "platform": {
      "ext-handlersocket": "5.6"
    },
    "sort-packages": true
  }
}
